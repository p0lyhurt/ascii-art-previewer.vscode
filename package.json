{
  "name": "ascii-art-previewer",
	"displayName": "ASCII Art Previewer",
	"description": "Previews ASCII art using MIRC colors.",
	"version": "0.0.1",
	"repository": "https://github.com/p0lyhurt/ascii-art-previewer.vscode",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "onCommand:ascii-art-previewer.show"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ascii-art-previewer.show",
				"title": "Show ASCII Art Previewer"
			}
		],
		"keybindings": [
			{
				"command": "ascii-art-previewer.show",
				"key": "Ctrl+K A"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
